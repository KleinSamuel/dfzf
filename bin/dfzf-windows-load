#!/usr/bin/env bash
dfzf-exec -t get_tree | jq -r '
    .. | objects | select(.marks[0] != null) | select(.app_id != "dfzf-popup") |
    {
        "id": .id,
	      "name": .name,
        "marks": .marks[0],
	"app_id": (.app_id // (if .shell == "xwayland" then .window_properties.class else empty end)[0:8] | gsub("^"; "[") | gsub("$"; "]     "))
    } |
  "\(.marks) \(.app_id[0:10]) \(.name) #\(.id)"
' |sort -nr|sed 's/_prev-[0-9]*//'|sed 's/â€” Mozilla Firefox //'
