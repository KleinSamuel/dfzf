#!/usr/bin/env bash
swaymsg -t get_tree | jq -r '
    .. | objects | select(.marks[0] != null) |
    {
        "id": .id,
	"name": .name,
        "marks": .marks[0],
        "app_id": .app_id
    } |
  "\(.marks) [\(.app_id[0:8])] \(.name) #\(.id)"
' |sort -nr|sed 's/_prev-[0-9]*//'|sed 's/â€” Mozilla Firefox //'
